{
  "coc.preferences.hoverTarget": "float",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "disableWorkspaceFolders": true,
      "filetypes": ["go"]
    }
  },
  "prettier.eslintIntegration": true,
  "suggest.timeout": 5000,
  "coc.preferences.useQuickfixForLocations": true
}
